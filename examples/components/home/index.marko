class {
  onCreate(input) {
    this.state = {
      todos: input.resolved.data || [],
    }
    this.state.todos = this.state.todos.filter((e, i) => i < 10)
  }
}
h1 -- Home example
h2 -- Tasks
ul
  for|todo| of=state.todos
    li style={ 'text-decoration': todo.completed ? 'line-through' : 'none' }
      -- ${ todo.title }


